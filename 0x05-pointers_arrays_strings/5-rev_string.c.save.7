#include <stdio.h>
/**
 * rev_string - writes the character c to stdout
 * @c: The character to print
 *
 * Return: On success 1.
 * On error, -1 is returned, and errno is set appropriately.
 */
void rev_string(char *s)
{
	int a = 0, b = 9,d;
char gree[10];

while (gree[b] != '\0')
	{
		gree[b] = s[a-b];
b--;
	}
printf("%s",gree);

	for (d = 0; d <= b; d++)
	{
		s[d] = gree[d];
	}

printf("\n");
}

