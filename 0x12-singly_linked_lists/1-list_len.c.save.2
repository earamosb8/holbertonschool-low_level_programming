#include <stdio.h>
#include "lists.h"
 * print_list - Print all the elements of a list_t list
 * @h: The list
 *
 * Return: The number of nodes
 */
size_t list_len(const list_t *h)
{
	unsigned long elements = 0;

	if (h == NULL)
	{
		return (0);
	}
	while (h != NULL)
	{
	h = h->next;
	elements++;
	}
return (elements);
}
